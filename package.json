{
	"name": "@biomejs/version-utils",
	"type": "module",
	"version": "0.1.1",
	"packageManager": "bun@1.1.0",
	"author": {
		"name": "Nicolas Hedger",
		"email": "nicolas@hedger.ch"
	},
	"license": "MIT",
	"homepage": "https://github.com/biomejs/version-utils#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/biomejs/version-utils.git"
	},
	"bugs": {
		"url": "https://github.com/biomejs/version-utils/issues"
	},
	"keywords": ["biome", "version", "utils"],
	"sideEffects": false,
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"typesVersions": {
		"*": {
			"*": ["./dist/*", "./dist/index.d.ts"]
		}
	},
	"files": ["dist"],
	"devDependencies": {
		"@types/bun": "^1.0.12",
		"@types/node": "^20.12.2",
		"@types/semver": "^7.5.8",
		"@types/yaml": "^1.9.7",
		"git-cliff": "^2.2.0",
		"lefthook": "^1.6.7",
		"tsup": "^8.0.2",
		"typescript": "^5.4.3"
	},
	"dependencies": {
		"semver": "^7.6.0",
		"undici": "^6.10.2",
		"yaml": "^2.4.1"
	},
	"scripts": {
		"build": "tsup"
	},
	"publishConfig": {
		"access": "public",
		"provenance": true
	}
}
